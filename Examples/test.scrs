@unicode
import 'scratch/looks';
import 'scratch/operators';
on start {
    if(fromUnicode(65) == "A") {
        sayTimed("fromUnicode works!", 5);
    }
    var converted = unicodeOf("ะช");
    sayTimed("unicodeOf of ะช is: " + toString(converted), 5);
    
    if("hey" == "heY") {
        throw "something went horribly wrong";
    }
    else {
        sayTimed("unicode string comparison works!", 5);
    }
}